{
	"name": "baraja-core/doctrine-fulltext-search",
	"description": "Entity search engine, extremely easy to use.",
	"homepage": "https://github.com/baraja-core/doctrine-fulltext-search",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.4.0",
		"nette/caching": "^3.0",
		"nette/utils": "^3.0",
		"doctrine/orm": "^2.7",
		"doctrine/dbal": "^2.9",
		"ramsey/uuid": "^3.0"
	},
	"require-dev": {
		"nette/di": "^3.0",
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6",
		"symplify/easy-coding-standard": "^7.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
